{% extends 'PetkoparaCrudGeneratorBundle::base.html.twig' %}

{% block title %}
{{ parent() }} - managerTravelPlan
{% endblock %}

{% block body %}

<div id="top" class="row">
    <div class="page-header">
        <h2>View managerTravelPlan <span class="glyphicon glyphicon-eye-open" aria-hidden="true"></span> </h2>
    </div>
</div>

<div class="row">
    <div class="col-md-6">
        <p><strong>Objetives</strong></p>
        <p>
            {{ managerTravelPlan.objetives }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Departuredate</strong></p>
        <p>
            {% if managerTravelPlan.departureDate %}{{ managerTravelPlan.departureDate|date('Y-m-d') }}{% endif %}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Lapsed</strong></p>
        <p>
            {{ managerTravelPlan.lapsed }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>State</strong></p>
        <p>
            {{ managerTravelPlan.state }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Approval</strong></p>
        <p>
            {% if managerTravelPlan.approval %}Yes{% else %}No{% endif %}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Approvaldate</strong></p>
        <p>
            {% if managerTravelPlan.approvalDate %}{{ managerTravelPlan.approvalDate|date('Y-m-d H:i:s') }}{% endif %}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Approvalobservations</strong></p>
        <p>
            {{ managerTravelPlan.approvalObservations }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Reject</strong></p>
        <p>
            {% if managerTravelPlan.reject %}Yes{% else %}No{% endif %}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Rejectdate</strong></p>
        <p>
            {% if managerTravelPlan.rejectDate %}{{ managerTravelPlan.rejectDate|date('Y-m-d H:i:s') }}{% endif %}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Rejectreason</strong></p>
        <p>
            {{ managerTravelPlan.rejectReason }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Closed</strong></p>
        <p>
            {% if managerTravelPlan.closed %}Yes{% else %}No{% endif %}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Used</strong></p>
        <p>
            {% if managerTravelPlan.used %}Yes{% else %}No{% endif %}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Createdat</strong></p>
        <p>
            {% if managerTravelPlan.createdAt %}{{ managerTravelPlan.createdAt|date('Y-m-d H:i:s') }}{% endif %}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Updatedat</strong></p>
        <p>
            {% if managerTravelPlan.updatedAt %}{{ managerTravelPlan.updatedAt|date('Y-m-d H:i:s') }}{% endif %}
        </p>

    </div></div>

<hr/>


<div class="form-group">
    <a class="btn btn-default" href="{{ path('exit_managertravelplan_index') }}">
        <span class="glyphicon glyphicon-list" aria-hidden="true"></span>
        Back to list
    </a>
    <a class="btn btn-primary" href="{{ path('exit_managertravelplan_edit', { 'numberSlug':  managerTravelPlan.numberSlug }) }}">
        <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>
        Edit
    </a>
    <a class="btn btn-info" href="{{ path('exit_managertravelplan_new') }}">
        <span class="glyphicon glyphicon-plus" aria-hidden="true"></span>
        New
    </a>
    <form action="{{ path('exit_managertravelplan_edit', { 'numberSlug': managerTravelPlan.numberSlug }) }}" method="post" style="display: inline-block">
        <input type="hidden" name="_method" value="DELETE" />
        {{ form_widget(delete_form) }}
        <button class="btn btn-danger" type="submit" onclick="return confirm('Are you sure');">
            <span class="glyphicon glyphicon-remove" aria-hidden="true"></span>
            Delete
        </button>
    </form>
</div>


{% endblock %}

