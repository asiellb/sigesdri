{% extends 'DRIUsefulBundle:facultad:layout.facultad.html.twig' %}

{% block title %}{{ parent() }} | Nuevo {% endblock %}

{% block  page_breadcrumb %}
    {{ parent() }}
    <li>
        <span>Nuevo </span>
    </li>
{% endblock %}

{% block page_title %}
    {{ parent() }}
    <small>Nuevo </small>
{% endblock %}

{% block page_dashboard %}
<div id="top" class="row">
    <div class="page-header">
        <h2>New facultad <span class="glyphicon glyphicon-file" aria-hidden="true"></span> </h2>
    </div>
</div>

<div class="row">
    {{ form_start(form) }}
    {{ form_widget(form) }}
    <p>
        <button type="submit" class="btn btn-primary"> 
            Submit <span class="glyphicon glyphicon-ok-circle" aria-hidden="true"></span>
        </button>
    </p>


    {{ form_end(form) }}

    <hr/>
        <div class="form-group">
    <a class="btn btn-default" href="{{ path('facultad') }}">
        <span class="glyphicon glyphicon-list" aria-hidden="true"></span>
        Back to list
    </a>
            </div>

</div>


{% endblock %}

